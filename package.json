{
  "name": "napi-bson-example",
  "version": "1.0.0",
  "description": "Template project for writing node package with napi-rs",
  "main": "api.js",
  "repository": "git@github.com:napi-rs/package-template.git",
  "license": "MIT",
  "files": [
    "index.d.ts",
    "index.js"
  ],
  "napi": {
    "name": "napi-bson-example"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "build": "napi build --platform --release",
    "debug": "napi build --platform",
    "clean": "cargo clean && rm -rf node_modules && rm -f *.node index.d.ts index.js Cargo.lock package-lock.json"
  },
  "dependencies": {
    "mongodb": "4.13.0"
  },
  "devDependencies": {
    "@napi-rs/cli": "2.18.4",
    "typescript": "5.5.4"
  }
}
